<h1> <%= @article.article_title %></h1>

<div>
  <% if !user_signed_in? %>
      ログインしてね
  <% elsif Like.find_by(article_id: params[:id],user_id: current_user.id) != nil %>
    <%= button_to 'いいね取り消す', article_like_path(@article), method: :delete %>
  <% else %>
    <%= button_to 'いいね', article_likes_path(@article) %>
  <% end %>
  いいね数:<%= @article.liked_users.count%>

</div>
<div>

    <%= @article.article_data %>
</div>
